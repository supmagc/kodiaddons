		<platform>all</platform>
		<summary lang="en_GB">Grey Movie Genre Icons Dutch</summary>
		<description lang="en_GB">Grey Movie Genre Icons Dutch</description>
		<source>https://github.com/supmagc/kodi-resource.images.moviegenreicons.grey.dutch</source>
		<email>supmagc at gmail dot com</email>
		<assets>
			<icon>resources/icon.png</icon>
		</assets>
	</extension>
